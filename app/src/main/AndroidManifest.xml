<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.d2edev.numbersprinter" >

    <application
        android:name="io.github.d2edev.numbersprinter.Core.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.Light" >
        <activity
            android:name="io.github.d2edev.numbersprinter.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service
            android:name="io.github.d2edev.numbersprinter.Core.TickerService"
            android:enabled="true"
            android:exported="false" >
        </service>
    </application>
    <uses-permission android:name="android.permission.VIBRATE" />
</manifest>
